#include <stdlib.h>
#include <string.h>
#include <stdio.h>
#include "lists.h"

/**
 * print_list - Entry point
 *
 * Description: A program that prints all the elements of a linked list
 * @h: pointer the the list_t structure
 * Return: Number of nodes
 */

size_t print_list(const list_t *h)
{

	size_t i;

	i = 0;
	while (h != NULL)
	{
		if (h->str == NULL)
			printf("[0] (nil)\n");
		else
			printf("[%d] %s\n", h->len, h->str);
		i++;
		h = h->next;
	}
	return i;
}
